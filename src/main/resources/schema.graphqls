type xw_legacyserver_entities_ChargeCode {
    id: ID!
    name: String!
    code: String
    description: String
    expired: Boolean
}

type xw_legacyserver_entities_TrackedTask {
    id: ID!
    notes: String
    chargeCodes: [xw_legacyserver_entities_ChargeCode] @relation(name:"CHARGED")
    timeBlocks: [xw_legacyserver_entities_TimeBlock!] @relation(name:"SPENT")
    createdAt: Float
    overtimeEnabled: Boolean
}

type xw_legacyserver_entities_TimeBlock {
    id: ID!
    startDate: Float
    trackedTask: xw_legacyserver_entities_TrackedTask
}

type Query {
    # Queries are generated by neo4j-graphql-java. Unless you need custom ones
}
