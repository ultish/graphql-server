type ChargeCode {
    id: ID!
    name: String!
    code: String
    description: String
    expired: Boolean
}

type TrackedTask {
    id: ID
    notes: String
    chargeCodes: [ChargeCode] @relation(name:"CHARGED")
    #    #    timeBlocks: [TimeBlock!]
    createdAt: Float
    overtimeEnabled: Boolean
}

